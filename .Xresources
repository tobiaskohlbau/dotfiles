Xft.dpi: 96
Xft.lcdfilter: lcddefault
Xft.hinting: 1
Xft.hintstyle: hintfull
Xft.autohint: 0
Xft.rgba: rgb
Xft.antialias: 1

URxvt.perl-ext-common: default,url-select,resize-font

URxvt.colorUL: #98971a
URxvt.matcher.button: 1
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.url-select.underline: true
URxvt.url-select.launcher: /usr/bin/xdg-open
URxvt.keysym.M-u: perl:url-select:select_next

URxvt.iso14755: false
URxvt.iso14755_52: false

URxvt.urgentOnBell: true
XTerm*bellIsUrgent: true
URxvt.cursorBlink: false
URxvt.transparent: false
URxvt.shading:          20
URxvt.scrollBar:        false
URxvt.saveLines:        4096
URxvt.mouseWheelScrollPage: true

ratmenu.style: dreary
ratmenu.font: fixed
ratmenu.bgcolor: #222222
ratmenu.fgcolor: #99ff00

URxvt.borderless: true
URxvt.geometry: 120x40

/* fonts */
URxvt.font:         xft:Inconsolata for Powerline:pixelsize=16,style=Medium xft:Symbola:pixelsize=19,style=Regular
URxvt.boldFont:     xft:Inconsolata for Powerline:style=Bold,pixelsize=16 xft:Symbola:pixelsize=19,style=Regular
Emacs.font:	    Inconsolata for Powerline:pixelsize=16:antialise:true
Emacs.fontBackendc: xft

*faceSize: 10

